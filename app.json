{
  "expo": {
    "name": "Fut Gamblers",
    "slug": "FG",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/logo_initial.png",
      "resizeMode": "contain",
      "backgroundColor": "#09ad00"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.futgamblers.futgamblers"
    },
    "android": {
      "package": "com.futgamblers.futgamblers",
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash.png",
        "backgroundColor": "#09ad00"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
